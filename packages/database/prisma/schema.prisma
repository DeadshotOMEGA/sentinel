  @@index([adminUserId, createdAt(sort: Desc)], map: "idx_audit_log_admin_created")
  @@index([createdAt(sort: Desc)], map: "idx_audit_log_created")
  @@index([entityType, entityId], map: "idx_audit_log_entity")
  @@map("audit_log")
}

